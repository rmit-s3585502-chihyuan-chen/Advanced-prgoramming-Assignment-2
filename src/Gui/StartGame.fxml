<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gui.StartGame">
   <children>
      <TextArea fx:id="gameInfo" layoutX="42.0" layoutY="113.0" prefHeight="213.0" prefWidth="438.0" />
      <Button fx:id="start" layoutX="200.0" layoutY="372.0" mnemonicParsing="false" onAction="#startGame" prefHeight="36.0" prefWidth="100.0" text="Start" />
      <Label layoutX="42.0" layoutY="46.0" prefHeight="50.0" prefWidth="112.0" text="Game Info:">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="home" layoutX="42.0" layoutY="372.0" mnemonicParsing="false" onAction="#home" prefHeight="36.0" prefWidth="100.0" text="Home" />
      <Button fx:id="exit" layoutX="366.0" layoutY="372.0" mnemonicParsing="false" onAction="#exit" prefHeight="36.0" prefWidth="100.0" text="Exit" />
   </children>
</AnchorPane>
